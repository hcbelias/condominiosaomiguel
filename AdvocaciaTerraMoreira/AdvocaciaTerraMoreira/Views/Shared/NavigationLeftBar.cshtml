<script type="text/javascript">
    function ValidaDados() {
        if ($('#usuario_integra').val() == "") {
            alert("O campo usuário é obrigatório");
            return false;
        }
        if ($('#senha_integra').val() == "") {
            alert("O campo senha é obrigatório");
            return false;
        }
        return true;
    }
</script>


