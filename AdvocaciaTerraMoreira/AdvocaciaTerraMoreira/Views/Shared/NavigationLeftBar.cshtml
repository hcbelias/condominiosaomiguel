
<nav id="nav_categories" class="nav_list">
    <form class="form-horizontal" role="form" action="http://www.api.adv.br/integra/login1.asp" target="_blank" method="post" onsubmit="return ValidaDados()">
        @Html.Partial("LoginFormSmall")
    </form>
