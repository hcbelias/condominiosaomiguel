@{
    var azulclaro = "#83B5B3";
    var azulescuro = "#076B68";
    var verdeclaro = "#84BA49";
    var verdemaisclaro = "#C1DCA4";
    var azulpreto = "#022B28";
    var inicio = verdeclaro;
    var fim = azulescuro;
}

<div class="container">
    <div class="header-content">
        <div class="row-fluid">

            <div class="span5">
