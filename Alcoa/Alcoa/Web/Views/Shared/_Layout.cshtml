@using System.Web.UI.WebControls
<html>
<head>
    <title>@ViewBag.Title</title>
    <link  href="@Url.Content(Util.Constants.URL_BOOTSTRAP_CSS)" rel="stylesheet" type="text/CSS"/>
    <link href="@Url.Content("~/Content/datetimepicker/css/bootstrap-datetimepicker.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/bootstrap/css/bootstrap-responsive.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/bootstrap-helpers/css/bootstrap-formhelpers.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/content.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/bootstrap-fileupload/bootstrap-fileupload.min.css")" rel="stylesheet"type="text/css" />
    <script   src="@Url.Content(Util.Constants.URL_JQUERY_JS)" type="text/javascript"></script>
    <script  src="@Url.Content(Util.Constants.URL_BOOTSTRAP_JS)" type="text/javascript"></script>
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
    <script  src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/datetimepicker/js/bootstrap-datetimepicker.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/bootstrap-helpers/js/bootstrap-formhelpers-phone.format.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/bootstrap-helpers/js/bootstrap-formhelpers-phone.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/bootstrap-helpers/js/bootstrap-formhelpers-selectbox.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/bootstrap-fileupload/bootstrap-fileupload.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/site.js")" type="text/javascript"></script>
</head>
<body>
    <div class="loading-progress">
        <div class="progress progress-striped active">
            <div class="bar" style="width: 100%">
            </div>
        </div>
    </div>
    <div id="wrap">
        @Html.Partial("Header")
        <div class="container">
            @RenderBody()
            <div id="push">
            </div>
        </div>
    </div>
    @RenderSection("Footer", false)
</body>
<script type="text/javascript">
    $(document).ready(function () {
        $("form").submit(function () {
            $('body').addClass("loading");
        });
        $.ajaxSetup({
            beforeSend: function () {

                $('body').addClass("loading");
            },
            complete: function () {

                $('body').removeClass("loading");
            }
        });
        validateData();
    });
</script>
</html>
