<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"></meta>
    <title>@ViewBag.Title</title>
    <link href="../../Content/bootstrap/css/bootstrap.css" rel="stylesheet" type="text/css" />
    <link href="../../Content/condominio-sao-miguel.css" rel="stylesheet" type="text/css" />
    <link href="../../Content/bootstrap/css/bootstrap-responsive.css" rel="stylesheet"
        type="text/css" />
</head>
<body>
    @Html.Partial("_LoadingProgress")
    <div class="header">
        @Html.Partial("_Header")
    </div>
    <div class="body">
        <div class="container">
            <div style="background-color: white;" class="row-fluid">
                <div class="span12" style="padding: 12px;" id='body-content'>
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
    <div class="footer">
        @Html.Partial("_Footer")
    </div>
    <script src="../../Scripts/jquery-1.8.2.min.js" type="text/javascript"></script>
    <script src="../../Content/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="../../Scripts/condominio-sao-miguel.js" type="text/javascript"></script>
    <script type="text/javascript">
        $(document).ready(function () {


        });



        var _gaq = _gaq || [];
        _gaq.push(['_setAccount', 'UA-36484766-1']);
        _gaq.push(['_trackPageview']);

        (function () {
            var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
            ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
            var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
        })();

	    
    </script>
</body>
</html>
